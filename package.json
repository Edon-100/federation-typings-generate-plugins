{
  "name": "federation-typings-generate-plugins",
  "version": "3.1.1",
  "description": "Webpack plugin to stream typescript for module federation apps/components",
  "license": "MIT",
  "type": "commonjs",
  "keywords": [
    "module-federation",
    "typescript",
    "remote types",
    "federated types"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Prasanna Mestha <prasanna@privjs.com> (https://github.com/prasannamestha)",
  "contributors": [
    "Zack Jackson <zackary.l.jackson@gmail.com> (https://github.com/ScriptedAlchemy)",
    "Pavan Divi <pavan.divi@outlook.com> (https://github.com/pavandv)"
  ],
  "peerDependencies": {
    "next": "*",
    "react": "*",
    "react-dom": "*",
    "typescript": "*",
    "vue-tsc": "^1.0.24",
    "webpack": "^5.75.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "vue-tsc": {
      "optional": true
    }
  },
  "exports": {
    ".": {
      "require": "./src/index.js",
      "types": "./src/index.d.ts"
    }
  },
  "dependencies": {
    "axios": "1.6.0",
    "lodash.get": "4.4.2",
    "node-fetch": "2.6.9",
    "tapable": "2.2.1",
    "util": "0.11.1",
    "@module-federation/utilities": "3.0.0",
    "webpack-sources": "3.2.3"
  },
  "main": "./src/index.js"
}
